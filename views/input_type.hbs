<header class="header">公司产品分类维护</header>
<section class="section common-form">
	<form class="form-horizontal js-put-form">
		<input type="hidden" name="id" value="{{typeId}}">
        <div class="hide js-form-type" data-type="3"></div>
		<div class="form-group">
			<label for="name" class="col-sm-2 control-label">产品分类</label>
			<div class="col-sm-6">
				<input type="text" class="form-control" id="name" placeholder="请输入招聘职位" name="typeName" value="{{typeName}}">
			</div>
            <div class="col-sm-4">
                <button type="submit" class="btn btn-primary">保存</button>
				<a href="/admin" class="btn btn-default">取消</a>
            </div>
		</div>
	</form>
</section>
